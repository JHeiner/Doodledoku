
{
"name": "Doodle",
"version": "0.1",
"description": "Doodle on stuff.",
"manifest_version": 2,
"icons": {
    "16": "pencil-16.png",
    "48": "pencil-48.png",
    "128": "pencil-128.png" },
"permissions": [ "tabs", "<all_urls>" ],
"background": { "page": "background.html" },
"browser_action": {
	"default_icon": "pencil-16.png",
	"default_title": "Toggle Doodle" },
"options_page": "options.html"
}

